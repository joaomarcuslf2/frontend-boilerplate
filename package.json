    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "eslint": "^2.7.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-react": "^6.4.1",
    "exports-loader": "~0.6.2",
    "expose-loader": "~0.6.0",
    "fetch-mock": "^5.6.2",
    "flow-bin": "^0.34.0",
    "foreman": "^3.0.1",
    "fs-extra": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-clean-css": "^2.0.13",
    "gulp-concat": "^2.6.1",
    "gulp-cssbeautify": "^0.1.3",
    "gulp-csscomb": "^3.0.8",
    "gulp-imagemin": "^3.1.1",
    "gulp-mocha": "^3.0.1",
    "gulp-sass": "^2.3.2",
    "gulp-util": "^3.0.7",
    "imports-loader": "~0.6.3",
    "mocha": "^2.5.3",
    "surge": "^0.18.0",
    "webpack": "~1.4.13",
    "webpack-dev-server": "^1.16.2"
  },
  "scripts": {
    "start": "npm init && npm install --silent && gulp run:build && gulp advice:server",
    "server": "node server.js",
    "test": "gulp run:test",
    "dev": "nf start",
    "build": "gulp run:build",
    "build:js": "webpack --minimize",
    "bundle:js": "webpack",
    "build:scss": "gulp build:scss",
    "bundle:scss": "gulp bundle:scss",
    "dev-server": "webpack-dev-server --hot --inline --color --port 8000 --host 0.0.0.0 --output-public-path=/build/ --content-base ./",
    "watch:js": "webpack --config ./webpack.config.js --watch --colors",
    "deploy:js": "webpack --minimize --output-path deploy/build",
    "deploy": "gulp pre:deploy && npm run deploy:js && gulp deploy && npm run publish",
    "increse:major": "gulp increase:major",
    "increse:minor": "gulp increase:minor",
    "increse:release": "gulp increase:release",
    "publish": "surge -p deploy -d project-name.surge.sh"
  },
