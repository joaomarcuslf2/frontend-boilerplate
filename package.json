{
  "name": "project-name",
  "version": "0.0.0",
  "description": "Personal Front-end boilerplate",
  "main": "app/app.js",
  "engines": {
    "node": "6.3.1",
    "npm": "4.0.2"
  },
  "dependencies": {
    "bulma": "^0.2.3",
